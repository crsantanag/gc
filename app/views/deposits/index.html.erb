<p style="color: green"><%= notice %></p>

<% content_for :title, "Deposits" %>

<h1>Deposits</h1>

<div id="deposits">
  <% @deposits.each do |deposit| %>
    <%= render deposit %>
    <p>
      <%= link_to "Show this deposit", deposit %>
    </p>
  <% end %>
</div>

<%= link_to "New deposit", new_deposit_path %>
